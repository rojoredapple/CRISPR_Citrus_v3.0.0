<p id="notice"><%= notice %></p>

<h1>Gene Athals</h1>

<table>
  <thead>
    <tr>
      <th>Gene id</th>
      <th>Orthogroup genes athal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gene_athals.each do |gene_athal| %>
      <tr>
        <td><%= gene_athal.Gene_ID %></td>
        <td><%= gene_athal.Orthogroup_genes_Athal %></td>
        <td><%= link_to 'Show', gene_athal %></td>
        <td><%= link_to 'Edit', edit_gene_athal_path(gene_athal) %></td>
        <td><%= link_to 'Destroy', gene_athal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gene Athal', new_gene_athal_path %>
