<p id="notice"><%= notice %></p>

<h1>Gene Csins</h1>

<table>
  <thead>
    <tr>
      <th>Gene id</th>
      <th>Orthogroup genes csin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gene_csins.each do |gene_csin| %>
      <tr>
        <td><%= gene_csin.Gene_ID %></td>
        <td><%= gene_csin.Orthogroup_genes_Csin %></td>
        <td><%= link_to 'Show', gene_csin %></td>
        <td><%= link_to 'Edit', edit_gene_csin_path(gene_csin) %></td>
        <td><%= link_to 'Destroy', gene_csin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gene Csin', new_gene_csin_path %>
